  "scripts": {
    "postbuild": "npm run sitemap",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "g:t": "npm run g:sb && npm run g:spy",
    "g:spy": "graphql-codegen -c codegen.shopify.js",
    "g:sb": "npm run storyblok-json-schema && npm run storyblok-json-types",
    "storyblok-json-schema": "storyblok pull-components --space 207492 --path=./libs/shared/schema/src/lib/storyblok/",
    "storyblok-json-types": "storyblok-generate-ts source=./libs/shared/schema/src/lib/storyblok/components.207492.json target=./libs/shared/schema/src/lib/storyblok/storyblok-types.ts"
  },
  "private": true,

    "gsap": "^3.11.5",
    "lodash.isempty": "^4.4.0",
    "next": "13.1.1",
    "next-i18next": "^13.2.2",
    "next-sitemap": "^4.0.6",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "18.2.0",
    "tslib": "^2.3.0"
  },
